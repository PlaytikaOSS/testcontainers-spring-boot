{
  "properties": [
    {
      "name": "embedded.mongodb.enabled",
      "type": "java.lang.String",
      "defaultValue": "true"
    },
    {
      "name": "embedded.mongodb.dockerImage",
      "type": "java.lang.String",
      "defaultValue": "mongo:6.0.4-focal"
    },
    {
      "name": "embedded.mongodb.host",
      "type": "java.lang.String",
      "defaultValue": "localhost"
    },
    {
      "name": "embedded.mongodb.port",
      "type": "java.lang.Integer",
      "description": "The container internal port. Will be overwritten with mapped port.",
      "defaultValue": "27017"
    },
    {
      "name": "embedded.mongodb.database",
      "type": "java.lang.String",
      "defaultValue": "test"
    },
    {
      "name": "embedded.mongodb.username",
      "type": "java.lang.String"
    },
    {
      "name": "embedded.mongodb.password",
      "type": "java.lang.String"
    }
  ]
}
